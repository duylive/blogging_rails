<h1>All Posts</h1>
<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Created at</th>
    <th>Updated at</th>
  </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <td><%= link_to post.title, post_path(post) %></td>
      <td><%= post.content %></td>
      <td><%= post.created_at.strftime("%B %d, %Y") %></td>
      <td><%= post.updated_at.strftime("%B %d, %Y") %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<p><%= link_to "New Post", new_post_path %></p>
