<h1>Users</h1>
<table>
  <thead>
  <tr>
    <th>Email</th>
    <th>Username</th>
    <th>Role</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td>
        <%= link_to "Edit", edit_admin_user_path(user) %>
        <%= link_to "Delete", admin_user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this user?" } %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
